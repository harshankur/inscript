{
    "name": "blog-inscript",
    "version": "1.0.0",
    "main": "server.js",
    "type": "module",
    "scripts": {
        "server": "node --watch server.js",
        "client": "vite",
        "setup": "node scripts/setup-inscript.js",
        "dev": "npm run setup && concurrently \"npm run server\" \"npm run client\"",
        "build": "vite build",
        "preview": "vite preview",
        "publish": "npm run setup && vite build --mode readonly && node scripts/generate-data.js && node scripts/generate-sitemap.js"
    },
    "dependencies": {
        "@tiptap/extension-color": "^2.27.1",
        "@tiptap/extension-highlight": "^2.27.1",
        "@tiptap/extension-image": "^2.1.13",
        "@tiptap/extension-subscript": "^2.27.1",
        "@tiptap/extension-superscript": "^2.27.1",
        "@tiptap/extension-text-align": "^2.27.1",
        "@tiptap/extension-text-style": "^2.27.1",
        "@tiptap/extension-underline": "^2.27.1",
        "@tiptap/pm": "^2.1.13",
        "@tiptap/react": "^2.1.13",
        "@tiptap/starter-kit": "^2.1.13",
        "axios": "^1.6.2",
        "concurrently": "^9.2.1",
        "cors": "^2.8.5",
        "diff": "^8.0.2",
        "dotenv": "^17.2.3",
        "express": "^4.18.2",
        "fs-extra": "^11.2.0",
        "gray-matter": "^4.0.3",
        "lucide-react": "^0.562.0",
        "marked": "^17.0.1",
        "multer": "^1.4.5-lts.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "turndown": "^7.1.2"
    },
    "devDependencies": {
        "@vitejs/plugin-react": "^4.2.1",
        "autoprefixer": "^10.4.16",
        "postcss": "^8.4.32",
        "tailwindcss": "^3.4.0",
        "vite": "^5.0.0"
    }
}